{
  "name": "api-modeloo",
  "version": "1.0.0",
  "description": "Api Modelo",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--max_old_space_size=4096 jest --runInBand --detectOpenHandles --forceExit",
    "dev": "ts-node-dev -r tsconfig-paths/register --poll --inspect --transpile-only --ignore-watch node_modules --respawn src/infraestructure/http/server.ts",
    "maven": "node-java-maven",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "npm run build && node -r tsconfig-paths/register dist/infraestructure/http/server.js"
  },
  "java": {
    "dependencies": [
      {
        "groupId": "net.sourceforge.jtds",
        "artifactId": "jtds",
        "version": "1.3.1"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "API_MODELO"
  },
  "keywords": [
    "sftp"
  ],
  "author": "Guilherme Cordeiro",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.18.6",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-prometheus": "^0.206.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@types/axios": "^0.9.36",
    "axios": "^1.12.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "light-node-metrics": "^1.0.4",
    "reflect-metadata": "^0.2.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node-cron": "^3.0.11",
    "tsyringe": "^4.8.0"
  }
}
